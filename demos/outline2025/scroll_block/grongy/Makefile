sroad000.zx02:	sroad000.big
	$(ZX02) -f sroad000.big sroad000.zx02

sroad000.big:	00001.gr 00003.gr 00005.gr 00007.gr \
		00009.gr 00011.gr 00013.gr 00015.gr
	cat 00001.gr > sroad000.big
	cat 00003.gr >> sroad000.big
	cat 00005.gr >> sroad000.big
	cat 00007.gr >> sroad000.big
	cat 00009.gr >> sroad000.big
	cat 00011.gr >> sroad000.big
	cat 00013.gr >> sroad000.big
	cat 00015.gr >> sroad000.big

